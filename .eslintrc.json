{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"plugins": ["prettier"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"rules": {
		"no-unused-vars": "warn",
		"prettier/prettier": "warn"
	}
}
